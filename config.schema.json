    "allow_overlapping_negatives": {
      "default": true,
      "title": "Allow Overlapping Negatives",
      "type": "boolean"
    },
    "log_rel_class_weights": {
      "default": false,
      "title": "Log Rel Class Weights",
      "type": "boolean"
    },
    "grad_accumulate": {
      "default": 1,
      "title": "Grad Accumulate",
      "type": "integer"
    },
    "loss": {
      "anyOf": [
        {
          "$ref": "#/$defs/LossCfg"
        },
        {
          "type": "null"
        }
      ],
      "default": null
    },
    "extractor": {
      "default": {
        "type": "fasterrcnn",
        "stage": "0",
        "backbone": "resnet50"
      },
      "discriminator": {
        "mapping": {
          "convnext": "#/$defs/ExtractorCfg_ConvNeXt",
          "dinov2": "#/$defs/ExtractorCfg_Dinov2",
          "fasterrcnn": "#/$defs/ExtractorCfg_FasterRCNN",
          "hrnet": "#/$defs/ExtractorCfg_HRNet",
          "mask2former": "#/$defs/ExtractorCfg_Mask2Former",
          "resnet": "#/$defs/ExtractorCfg_ResNet"
        },
        "propertyName": "type"
      },
      "oneOf": [
        {
          "$ref": "#/$defs/ExtractorCfg_FasterRCNN"
        },
        {
          "$ref": "#/$defs/ExtractorCfg_Mask2Former"
        },
        {
          "$ref": "#/$defs/ExtractorCfg_Dinov2"
        },
        {
          "$ref": "#/$defs/ExtractorCfg_HRNet"
        },
        {
          "$ref": "#/$defs/ExtractorCfg_ConvNeXt"
        },
        {
          "$ref": "#/$defs/ExtractorCfg_ResNet"
        }
      ],
      "title": "Extractor"
    },
    "architecture": {
      "allOf": [
        {
          "$ref": "#/$defs/ArchCfg_DaniFormer"
        }
      ],
      "default": {
        "transformer_depth": 6,
        "embed_dim": 384,
        "patch_size": 8,
        "use_semantics": false,
        "use_masks": false,
        "bg_ratio_strategy": "total",
        "concat_mask": false,
        "norm_tokens": false,
        "use_patch_tokens_for_node": false,
        "encode_coords": false,
        "classification_token": true
      }
    },
    "data": {
      "allOf": [
        {
          "$ref": "#/$defs/DataCfg"
        }
      ],
      "default": {
        "source": "psg"
      }
    }
