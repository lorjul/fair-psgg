{
  "$defs": {
    "ArchCfg_DaniFormer": {
      "properties": {
        "transformer_depth": {
          "default": 6,
          "title": "Transformer Depth",
          "type": "integer"
        },
        "embed_dim": {
          "default": 384,
          "title": "Embed Dim",
          "type": "integer"
        },
        "patch_size": {
          "default": 8,
          "title": "Patch Size",
          "type": "integer"
        },
        "use_semantics": {
          "default": false,
          "title": "Use Semantics",
          "type": "boolean"
        },
        "use_masks": {
          "default": false,
          "title": "Use Masks",
          "type": "boolean"
        },
        "bg_ratio_strategy": {
          "default": "total",
          "enum": [
            "total",
            "indiv",
            "sum1",
            "onoff"
          ],
          "title": "Bg Ratio Strategy",
          "type": "string"
        },
        "concat_mask": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "const": "ignore",
              "enum": [
                "ignore"
              ],
              "type": "string"
            }
          ],
          "default": false,
          "title": "Concat Mask"
        },
        "norm_tokens": {
          "default": false,
          "title": "Norm Tokens",
          "type": "boolean"
        },
        "use_patch_tokens_for_node": {
          "default": false,
          "title": "Use Patch Tokens For Node",
          "type": "boolean"
        },
        "encode_coords": {
          "default": false,
          "title": "Encode Coords",
          "type": "boolean"
        },
        "classification_token": {
          "default": true,
          "title": "Classification Token",
          "type": "boolean"
        }
      },
      "title": "ArchCfg_DaniFormer",
      "type": "object"
    },
    "DataCfg": {
      "properties": {
        "source": {
          "default": "psg",
          "enum": [
            "psg",
            "o365",
            "vg-ietrans",
            "psg-coco"
          ],
          "title": "Source",
          "type": "string"
        }
      },
      "title": "DataCfg",
      "type": "object"
    },
    "ExtractorCfg_ConvNeXt": {
      "properties": {
        "type": {
          "const": "convnext",
          "default": "convnext",
          "enum": [
            "convnext"
          ],
          "title": "Type",
          "type": "string"
        },
        "variant": {
          "default": "large.fb_in22k_ft_in1k_384",
          "title": "Variant",
          "type": "string"
        }
      },
      "title": "ExtractorCfg_ConvNeXt",
      "type": "object"
    },
    "ExtractorCfg_Dinov2": {
      "properties": {
        "type": {
          "const": "dinov2",
          "default": "dinov2",
          "enum": [
            "dinov2"
          ],
          "title": "Type",
          "type": "string"
        },
        "variant": {
          "default": "s",
          "enum": [
            "s",
            "b",
            "l",
            "g"
          ],
          "title": "Variant",
          "type": "string"
        }
      },
      "title": "ExtractorCfg_Dinov2",
      "type": "object"
    },
    "ExtractorCfg_FasterRCNN": {
      "properties": {
        "type": {
          "const": "fasterrcnn",
