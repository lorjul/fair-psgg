          "enum": [
            "fasterrcnn"
          ],
          "title": "Type",
          "type": "string"
        },
        "stage": {
          "default": "0",
          "enum": [
            "0",
            "1",
            "2",
            "3",
            "all"
          ],
          "title": "Stage",
          "type": "string"
        },
        "backbone": {
          "default": "resnet50",
          "enum": [
            "resnet50",
            "mobilenet"
          ],
          "title": "Backbone",
          "type": "string"
        }
      },
      "title": "ExtractorCfg_FasterRCNN",
      "type": "object"
    },
    "ExtractorCfg_HRNet": {
      "properties": {
        "type": {
          "const": "hrnet",
          "default": "hrnet",
          "enum": [
            "hrnet"
          ],
          "title": "Type",
          "type": "string"
        },
        "variant": {
          "const": "w32",
          "default": "w32",
          "enum": [
            "w32"
          ],
          "title": "Variant",
          "type": "string"
        }
      },
      "title": "ExtractorCfg_HRNet",
      "type": "object"
    },
    "ExtractorCfg_Mask2Former": {
      "properties": {
        "type": {
          "const": "mask2former",
          "default": "mask2former",
          "enum": [
            "mask2former"
          ],
          "title": "Type",
          "type": "string"
        },
        "checkpoint": {
          "default": "facebook/mask2former-swin-base-coco-panoptic",
          "title": "Checkpoint",
          "type": "string"
        },
        "features": {
          "default": "encoder",
          "enum": [
            "encoder",
            "decoder",
            "decoder_merged"
          ],
          "title": "Features",
          "type": "string"
        }
      },
      "title": "ExtractorCfg_Mask2Former",
      "type": "object"
    },
    "ExtractorCfg_ResNet": {
      "properties": {
        "type": {
          "const": "resnet",
          "default": "resnet",
          "enum": [
            "resnet"
          ],
          "title": "Type",
          "type": "string"
        },
        "name": {
          "default": "resnet101",
          "title": "Name",
          "type": "string"
        }
      },
      "title": "ExtractorCfg_ResNet",
      "type": "object"
    },
    "LRScheduleStep": {
      "properties": {
        "type": {
          "const": "step",
          "default": "step",
          "enum": [
            "step"
          ],
          "title": "Type",
          "type": "string"
        },
        "step_size": {
          "default": 10,
          "title": "Step Size",
          "type": "integer"
        },
        "factor": {
          "default": 0.1,
          "title": "Factor",
          "type": "number"
        }
      },
      "title": "LRScheduleStep",
      "type": "object"
    },
    "LossCfg": {
      "properties": {
        "rank_weight": {
          "default": 0.0,
          "title": "Rank Weight",
          "type": "number"
        },
        "interpolate_weights": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Interpolate Weights"
        }
      },
      "title": "LossCfg",
      "type": "object"
    },
    "_Augmentations": {
      "properties": {
        "train": {
          "items": {
            "anyOf": [
              {
                "type": "object"
              },
              {
                "type": "string"
              }
            ]
          },
          "title": "Train",
          "type": "array"
        },
        "val": {
          "items": {
            "anyOf": [
              {
                "type": "object"
              },
              {
                "type": "string"
              }
            ]
          },
          "title": "Val",
          "type": "array"
        }
      },
      "required": [
        "train",
        "val"
      ],
      "title": "_Augmentations",
      "type": "object"
    }
  },
  "properties": {
    "rel_weight": {
      "default": 0.8,
      "title": "Rel Weight",
      "type": "number"
    },
    "node_loss_weight": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Node Loss Weight"
    },
    "batch_size": {
      "default": 32,
      "title": "Batch Size",
      "type": "integer"
    },
    "rels_per_batch": {
      "default": 4096,
      "title": "Rels Per Batch",
      "type": "integer"
    },
    "lr": {
      "default": 0.001,
      "title": "Lr",
      "type": "number"
    },
    "lr_backbone": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Lr Backbone"
    },
    "lr_schedule": {
      "anyOf": [
        {
          "$ref": "#/$defs/LRScheduleStep"
        },
        {
          "type": "null"
        }
      ],
      "default": null
