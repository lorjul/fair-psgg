    "weight_decay": {
      "default": 0.01,
      "title": "Weight Decay",
      "type": "number"
    },
    "neg_ratio": {
      "default": 1.0,
      "title": "Neg Ratio",
      "type": "number"
    },
    "augmentations": {
      "anyOf": [
        {
          "$ref": "#/$defs/_Augmentations"
        },
        {
          "type": "null"
        }
      ],
      "default": null
